<?xml version="1.0" encoding="utf-8"?>

<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:shrinkColumns="1">

    <TableRow>
        <TextView
            android:id="@+id/nameDetailView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:text="Name"
            android:textStyle="bold"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
        <!--app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"-->

        <TextView
            android:id="@+id/name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:text="Name TextView"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
            <!--app:layout_constraintBottom_toTopOf="@+id/textView17"
            app:layout_constraintStart_toEndOf="@+id/textView17"-->


    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/genderDetailView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="10dp"
            android:text="Gender"
            android:textStyle="bold"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
        <!--app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView17"-->

        <TextView
            android:id="@+id/gender_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="15dp"
            android:text="Gender TextView"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
            <!--app:layout_constraintStart_toStartOf="@+id/capacity_tv"
            app:layout_constraintTop_toBottomOf="@+id/capacity_tv"-->
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/capDetailView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="10dp"
            android:text="Capacity"
            android:textStyle="bold"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
            <!--app:layout_constraintStart_toStartOf="@+id/nameDetailView"
            app:layout_constraintTop_toBottomOf="@+id/nameDetailView"-->

        <TextView
            android:id="@+id/capacity_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="15dp"
            android:text="Capacity TextView"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
            <!--app:layout_constraintStart_toStartOf="@+id/name_tv"
            app:layout_constraintTop_toBottomOf="@+id/name_tv"-->
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/phoneDetailView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="10dp"
            android:text="Phone"
            android:textStyle="bold"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
        <!--app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18"-->

        <TextView
            android:id="@+id/phone_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="15dp"
            android:text="Phone TextView"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
            <!--app:layout_constraintStart_toStartOf="@+id/address_tv"
            app:layout_constraintTop_toBottomOf="@+id/address_tv"-->
    </TableRow>

    <TableRow>
        <TextView
            android:id="@+id/addressDetailView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="10dp"
            android:text="Address"
            android:textStyle="bold"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />
            <!--app:layout_constraintStart_toStartOf="@+id/textView19"
            app:layout_constraintTop_toBottomOf="@+id/textView19" -->

        <TextView
            android:id="@+id/address_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginStart="15dp"
            android:inputType="textMultiLine"
            android:text="Address TextView"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextViewEdits" />
            <!--app:layout_constraintStart_toStartOf="@+id/latitiude_tv"
            app:layout_constraintTop_toBottomOf="@+id/latitiude_tv"-->
    </TableRow>

    <TableRow>

        <EditText
            android:id="@+id/reserve_number_box"
            android:labelFor="@id/reserve_number_box"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="30dp"
            android:inputType="number"
            android:text="0"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/reserve_btn"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="20dp"
            android:layout_marginTop="30dp"
            android:text="Reserve Spot(s)"
            tools:ignore="HardcodedText" />
    </TableRow>

    <TableRow>

        <Button
            android:id="@+id/release_bed_btn"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="20dp"
            android:background="@android:color/holo_red_light"
            android:text="Release Spot(s)"
            tools:ignore="HardcodedText" />
    </TableRow>

</TableLayout>

